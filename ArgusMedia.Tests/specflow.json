{
  "$schema": "https://specflow.org/specflow-config.json",
  "bindingCulture": {
    "language": "en-us"
  },
  "language": {
    "feature": "en"
  },
  "generator": {
    "allowDebugGeneratedFiles": true,
    "generateAsyncTests": true,
    "path": "Features",
    "skipParallelCodeGeneration": false
  },
  "trace": {
    "traceSuccessfulSteps": true,
    "traceTimings": true,
    "listener": "Console"
  },
  "dependencyInjection": {
    "dependencies": [
      {
        "type": "ArgusMedia.Tests.Steps.TestContext",
        "lifetime": "Scenario"
      }
    ],
    "plugins": [
      {
        "name": "SpecFlow.NUnit",
        "type": "Generator"
      },
      {
        "name": "SpecFlow.NUnit",
        "type": "Runtime"
      }
    ]
  }