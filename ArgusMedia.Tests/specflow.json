{
  "$schema": "https://specflow.org/specflow-config.json",
  "bindingCulture": {
    "language": "en-us"
  },
  "language": {
    "feature": "en"
  },
  "generator": {
    "allowDebugGeneratedFiles": true,
    "generateAsyncTests": true,
    "path": "Features",
    "skipParallelizableMarkerForTags": []
  },
  "runtime": {
    "obfuscate": false,
    "stopAtFirstError": false,
    "missingOrPendingStepsOutcome": "Pending",
    "detectAmbiguousMatches": true
  },
  "stepAssemblies": [],
  "trace": {
    "traceSuccessfulSteps": true,
    "traceTimings": true,
    "minTracedDuration": "0:0:0.1",
    "listener": "Console"
  },
  "plugins": [
    {
      "name": "SpecFlow.NUnit",
      "type": "Generator"
    },
    {
      "name": "SpecFlow.NUnit",
      "type": "Runtime"
    }
  ]
}